@page "/audit/listening1"
@using LearnOnline.Models.Entities
<div id="wp-products">
    <h2>Danh sách bài test kĩ năng Listening</h2>
    <ul id="list-products">
        @foreach (var item in Parts)
        {

            <div class="item">
                <a href="/audit/listening/@item.Id">
                    <img class="media-img" src="../image/@item.Image" style="height:100px;width:200px" alt="">
                </a>
                <div class="name">@item.Title</div>
            </div>
        }
    </ul>
</div>

@code {
     private static List<Part> Parts = new List<Part>();
}
